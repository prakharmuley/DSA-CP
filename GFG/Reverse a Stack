class Solution {
    public static void reverse(Stack<Integer> st)
    {
        if(st.isEmpty())
          return;
          int top=st.pop();
          reverse(st);
          Stack<Integer> temp=new Stack<>();
          while(!st.isEmpty()) temp.push(st.pop());
          
          st.push(top);
          while(!temp.isEmpty()) st.push(temp.pop());
          
    }
    public static void reverseStack(Stack<Integer> st) {
        // code here
        reverse(st);
        
    }
}
