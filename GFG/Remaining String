class Solution {
    public String printString(String s, char ch, int count) {
        // code here
         // code here
        String ans = "";
        int temp =0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i) == ch){
                temp++;
            }
            if(temp == count){
                ans = s.substring(i+1);
                return ans;
            }
        }
        return ans;
    }
}
