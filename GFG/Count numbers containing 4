class Solution {
      static boolean containsDigitFour(int n)
      {
          String s=String.valueOf(n);
          if(s.contains("4")) return true;
          
          return false;
      }
    public static int countNumberswith4(int n) {
        // code here
          int count = 0;
        for (int i = 1; i <= n; ++i) {
            if (containsDigitFour(i)) {
                count++;
            }
        }
        return count;
        
        
    }
}
