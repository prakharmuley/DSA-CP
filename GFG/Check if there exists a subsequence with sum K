// User function Template for Java

class Solution {
    public static boolean checkSubsequenceSum(int N, int[] arr, int K) {
        // code here
        if(K==0) return true;
        if(K<0||N<=0) return false;
        
        if (checkSubsequenceSum(N-1, arr, K-arr[N-1]))
            return true;

        return checkSubsequenceSum(N-1, arr, K);

        
    }
}
