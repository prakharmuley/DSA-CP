class Solution {
   public int[] getNoZeroIntegers(int n) {
       int i=1,j=n-1,t=n/2-1;
       while(j!=t){
           if(isValid(i,j)){
               return new int[]{i,j};
           }
           i++;j--;
       }
       return new int[]{-1,-1};
   }
   private boolean isValid(int i, int j){
       return !containsZero(i)&&!containsZero(j)?true:false;
   }
   private boolean containsZero(int i){
       while(i!=0){
           int temp = i%10;
           if(temp==0){
               return true;
           }
           i = i/10;
       }
       return false;
   }
}
