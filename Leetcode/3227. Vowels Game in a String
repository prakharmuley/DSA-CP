class Solution {
    public boolean doesAliceWin(String s) {
        int cnt=0;
        for(char c: s.toCharArray())
        {
            if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u') 
            {
                cnt++;
            }
        }
         if(cnt%2==1)
         {
            return true;
         }
         else if(cnt%2==0)
         {
            if(cnt==0) return false;
            else return true;
         }
         return false;
    }
}
