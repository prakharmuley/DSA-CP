class Solution {
    public void fun(int[] arr, int i, int n, List<List<Integer>> res, List<Integer> list)
    {
        if(i>=n){
          res.add(new ArrayList<>(list));
          return;
        }
        
            list.add(arr[i]);
            fun(arr,i+1,n,res,list);
            list.remove(list.size()-1);
            fun(arr,i+1,n,res,list);

          
    }

    public List<List<Integer>> subsets(int[] nums) {
        List<List<Integer>> res=new ArrayList<>();
        List<Integer> list=new ArrayList<>();
        int n=nums.length;
        fun(nums,0,n,res,list);
        return res;
    }
}
